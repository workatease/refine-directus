{
  "name": "@workatease/refine-directus",
  "description": "Refine Directus REST data provider. This is a wrapper around the Directus SDK to use in refine.",
  "version": "1.0.7",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": "git@github.com:workatease/refine-directus.git",
  "private": false,
  "keywords": [
    "directus",
    "directus9",
    "refine",
    "rest",
    "sdk"
  ],
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsup --watch --dts --format esm,cjs,iife --legacy-output",
    "build": "tsup --dts --format esm,cjs,iife --minify --legacy-output",
    "test": "jest --passWithNoTests --runInBand",
    "test-cov": "jest --passWithNoTests --runInBand --coverage",
    "prepare": "npm run build"
  },
  "author": "Pyll Gomez",
  "module": "dist/refine-directus.esm.js",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "jest": "^27.5.1",
    "jsonwebtoken": "^8.5.1",
    "ts-jest": "^27.1.4",
    "tsup": "^5.11.13",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@directus/sdk": "^9.11.0",
    "@pankod/refine-core": "3.16.2",
    "tslib": "^2.4.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": false,
    "platform": "browser"
  }
}